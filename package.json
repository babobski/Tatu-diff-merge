{
	"name": "tatu-diff",
	"displayName": "Tatu Diff Merge",
	"description": "Side by side diff merge",
	"keywords": [
		"diff",
		"merge",
		"diff-merge",
		"diffmerge"
	],
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#FF6160",
		"theme": "light"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/babobski/Tatu-diff-merge"
	},
	"publisher": "Babobski",
	"version": "0.1.6",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tatudiff.mergeWithClipboard",
		"onCommand:tatudiff.mergeWithFile",
		"onCommand:tatudiff.mergeWithFileOnDisk"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "TatuDiff",
			"properties": {
				"tatu-diff.theme": {
					"type": "string",
					"default": "",
					"markdownDescription": "The default color scheme, to see all themes in action visit [highlightjs](https://highlightjs.org/static/demo/) (note: base16 color schemes are not included)",
					"enum": [
						"none",
						"default",
						"A 11 Y Dark",
						"A 11 Y Light",
						"Agate",
						"An Old Hope",
						"Androidstudio",
						"Arduino Light",
						"Arta",
						"Ascetic",
						"Atom One Dark",
						"Atom One Dark Reasonable",
						"Atom One Light",
						"Brown Paper",
						"Codepen Embed",
						"Color Brewer",
						"Dark",
						"Devibeans",
						"Docco",
						"Far",
						"Foundation",
						"Github",
						"Github Dark",
						"Github Dark Dimmed",
						"Gml",
						"Googlecode",
						"Gradient Dark",
						"Gradient Light",
						"Grayscale",
						"Hybrid",
						"Idea",
						"Ir Black",
						"Isbl Editor Dark",
						"Isbl Editor Light",
						"Kimbie Dark",
						"Kimbie Light",
						"Lightfair",
						"Lioshi",
						"Magula",
						"Mono Blue",
						"Monokai",
						"Monokai Sublime",
						"Night Owl",
						"Nnfx Dark",
						"Nnfx Light",
						"Nord",
						"Obsidian",
						"Paraiso Dark",
						"Paraiso Light",
						"Pojoaque",
						"Purebasic",
						"Qtcreator Dark",
						"Qtcreator Light",
						"Rainbow",
						"Routeros",
						"School Book",
						"Shades Of Purple",
						"Srcery",
						"Stackoverflow Dark",
						"Stackoverflow Light",
						"Sunburst",
						"Tomorrow Night Blue",
						"Tomorrow Night Bright",
						"Vs",
						"Vs 2015",
						"Xcode",
						"Xt 256"
					]
				}
			}
		},
		"menus": {
			"editor/context": [
				{
					"command": "tatudiff.mergeWithClipboard",
					"when": "!isInDiffEditor && !editorReadonly"
				},
				{
					"command": "tatudiff.mergeWithFile",
					"when": "!isInDiffEditor && !editorReadonly"
				},
				{
					"command": "tatudiff.mergeWithFileOnDisk",
					"when": "!isInDiffEditor && !editorReadonly"
				}
			]
		},
		"commands": [
			{
				"command": "tatudiff.mergeWithClipboard",
				"title": "Tatu Diff: Diff/Merge with Clipboard"
			},
			{
				"command": "tatudiff.mergeWithFile",
				"title": "Tatu Diff: Diff/Merge with File"
			},
			{
				"command": "tatudiff.mergeWithFileOnDisk",
				"title": "Tatu Diff: Diff/Merge with File on Disk"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
